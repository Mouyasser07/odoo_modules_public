<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="dynamic_snippet_template" name="Dynamic Snippet">
        <section class="pt40 pb40">
            <div class="container dynamic_snippet_template">
                <h1 class="text-center">Good job</h1>
            </div>
            <div id="dynamic_snippet_carousel"/>
            <div class="table-responsive-md">
                <table class="table">
                    <caption>List of users</caption>
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">First</th>
                            <th scope="col">Last</th>
                            <th scope="col">Handle</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope="row">1</th>
                            <td>Mark</td>
                            <td>Otto</td>
                            <td>@mdo</td>
                        </tr>
                        <tr>
                            <th scope="row">2</th>
                            <td>Jacob</td>
                            <td>Thornton</td>
                            <td>@fat</td>
                        </tr>
                        <tr>
                            <th scope="row">3</th>
                            <td>Larry</td>
                            <td>the Bird</td>
                            <td>@twitter</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </section>
    </template>
</odoo>