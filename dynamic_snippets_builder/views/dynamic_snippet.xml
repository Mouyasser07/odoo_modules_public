<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="basic_snippet" inherit_id="website.snippets" name="Dynamic Snippet">
        <xpath expr="//div[@id='snippet_effect']//t[@t-snippet][last()]"
               position="after">
            <t t-snippet="dynamic_snippets_builder.dynamic_snippet_template" string="Dynamic Snippet"
               t-thumbnail="/dynamic_snippets_builder/static/dynamic/icon.png"/>
        </xpath>
    </template>
</odoo>